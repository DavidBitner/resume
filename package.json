{
  "name": "resume",
  "version": "1.0.0",
  "description": "Interactive resume",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass src/sass/main.scss src/css/style.css --watch",
    "compile:sass": "sass src/sass/main.scss src/css/style.comp.css --no-source-map",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "sass src/css/style.prefix.css src/css/style.css --style=compressed --no-source-map",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidBitner/resume.git"
  },
  "author": "David Bitner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidBitner/resume/issues"
  },
  "homepage": "https://github.com/DavidBitner/resume#readme",
  "dependencies": {
    "autoprefixer": "^10.4.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16"
  },
  "devDependencies": {
    "sass": "^1.97.2"
  }
}
